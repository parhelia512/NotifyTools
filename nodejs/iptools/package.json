{
  "name": "iptools",
  "version": "1.0.0",
  "author": "yippeesoft <yippeesoft@gmail.com>",
  "description": "iptools",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
     "build": "node .electron-vue/build.js",
    "babelWatch": "babel src/**/*.js -d dist -w -s inline",
    "buildd": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win --ia32"
  },
  "build": {
    "productName": "iptools",
    "appId": "org.simulatedgreg.electron-vue",
	"copyright":"LEON",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/logo.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "electron": ">=9.4.0",
    "axios": ">=0.18.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "global": "^4.3.2",
    "json-server": "^0.12.1",
    "net": "^1.0.2",
    "rxjs": "^5.5.6",
    "typescriptcollectionsframework": "^3.1.2",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^2.5.3",
    "vue-rx": "^5.0.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    //"electron": "^1.7.5",
    "electron": ">=7.2.4"
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "iview": "^2.9.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "url-loader": "^0.5.9",
    "vue-class-component": "^6.1.2",
    "vue-clipboard2": "0.0.6",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": ">=3.1.11",
    "webpack-hot-middleware": "^2.18.2"
  }
}
